@mixin gradient($color1, $color2) {
    background: -moz-linear-gradient(left, $color1 0%, $color2 10%, $color2 90%, $color1 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, right top, color-stop(0%,$color1), color-stop(10%,$color2), color-stop(90%,$color2), color-stop(100%,$color1)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(left,  $color1 0%,$color2 10%,$color2 90%,$color1 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(left, $color1 0%,$color2 10%,$color2 90%,$color1 100%); /* Opera 11.10+ */
    background: -ms-linear-gradient(left, $color1 0%,$color2 10%,$color2 90%,$color1 100%); /* IE10+ */
    background: linear-gradient(to right, $color1 0%,$color2 10%,$color2 90%,$color1 100%); /* W3C */
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='$color1', endColorstr='$color1',GradientType=1 ); /* IE6-8 */
}

.keyofferings {
    margin: 0 0 -25px 0;
    background: none;

    h1 {
        margin-top: 0;
        padding-top: 0.5em;
    }
    .keyofferings-list {
        overflow: hidden;
        margin: 0 auto 40px auto;
        padding: 0;
    }
    .keyofferings-list-item {
        float: left;
        margin: 0 90px 30px 0;
        display: block;

        &:nth-child(even) {
            margin-right: 0;
        }
        a {
            text-decoration: none;
            display: block;
            padding: 10px 0 6px 0;
            width: 405px;
            background: rgb(200,50,50);
            text-align: center;
            font-family: $sansFont;
            font-size: 1.6em;
            color: #fff;
            letter-spacing: 1px;
            -webkit-box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.4);
                    box-shadow: 0px 2px 4px 0px rgba(0, 0, 0, 0.4);

            &:hover, 
            &:active {
                -webkit-box-shadow: inset 0px 5px 5px 0px rgba(0, 0, 0, 0.2);
                        box-shadow: inset 0px 5px 5px 0px rgba(0, 0, 0, 0.2);
            }
        }

        a.engineering {
            background: #3a6665; /* Old browsers */
            background: url(data:image/svg+xml;base64,);
            @include gradient(#3a6665, #578e8c);
        }
        a.ideation {
            background: #d72b3f; /* Old browsers */
            background: url(data:image/svg+xml;base64,);
            @include gradient(#d72b3f, #ec5062);
        }
        a.innovation {
            background: #dcb007; /* Old browsers */
            background: url(data:image/svg+xml;base64,);
            @include gradient(#dcb007, #f2ca0d);
        }
        a.education {
            background: #444446; /* Old browsers */
            background: url(data:image/svg+xml;base64,);
            @include gradient(#444446, #57585a);
        }
        a.insights {
            background: #e78e22; /* Old browsers */
            background: url(data:image/svg+xml;base64,);
            @include gradient(#e78e22, #f2ad4b);
        }
        a.systhink {
            background: #82626d; /* Old browsers */
            background: url(data:image/svg+xml;base64,);
            @include gradient(#82626d, #a87b8b);
        }
    }
